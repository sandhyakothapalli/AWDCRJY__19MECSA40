import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner  sc=new Scanner(System.in);
		System.out.println("enter the array size");
		int  n=sc.nextInt();
		System.out.println("enter the element to print index");
		int targt=sc.nextInt();
		int arr[]=new int[n];
		System.out.println("enter the array eelements");
		for(int i=0;i<n;i++)
		arr[i]=sc.nextInt();
		for(int i=0;i<n;i++)
		{
		    if(targt==arr[i])
		    {
		    System.out.println(i);
		    }
		    else if(targt>arr[i] && targt<arr[i+1])
		    {
		        System.out.println(i+1);
		    }
		}
		
	}
}
uutput:
enter the array size
5 
enter the element to print index
3
enter the array eelements
1 2 4 5 6
2
