import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	String str=sc.next();
	char ch[]=str.toCharArray();
	Arrays.sort(ch);
	String str2="";
	for(int i=0;i<ch.length;i++)
	{
	    str2+=ch[i];
	}
	    System.out.print(str2);
	}
	
}
output;
