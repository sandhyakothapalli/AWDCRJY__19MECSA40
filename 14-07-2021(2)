import java.util.*; 
class frequencyofele 
{ 
public static void main(String[] args) 
{ 
Scanner sc=new Scanner(System.in); 
System.out.println(" Enter size "); 
int n=sc.nextInt(); 
int a[]=new int[n]; 
System.out.println("the array elements are");  
for(int i=0;i<n;i++) 
a[i]=sc.nextInt(); 
sc.nextLine().split(" "); 
System.out.println("enter the key"); 
int k=sc.nextInt(); 
noofelements(a,n,k); 
} 
static void noofelements(int a[],int n,int k)
 { 
int f=0; 
System.out.println("the frequency of element"); 
for(int i=0;i<n;i++) 
if(a[i]==k) 
f++; 
System.out.println(f+" "); 
} 
}
Output 
Enter size 5
Enter array 2 3 5 3 3
Enter the key 3
Frequency of element 3
