import java.util.*;
import java.lang.*;
class Solution {
    public static boolean isUgly(int n) {
        if(n<=0)
            return false;
        while(n%2==0)
            return true;
        while(n%3==0)
            return true;
        while(n%5==0)
            return true;
        if(n==1)
            return true;
        else 
            return false;
    }
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    System.out.println(isUgly(n));
}
}
