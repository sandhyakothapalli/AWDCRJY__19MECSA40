import java.util.*;
public class Main
{
	public static void main(String[] args) {
	   Scanner sc=new Scanner(System.in);
	   int arr[]={10,20,30,40,20,10};
	   int n=arr.length;
	   Map<Integer,Integer> h =new HashMap<> ();
	   for(int i=0;i<n;i++)
	   {
	       if(h.containsKey(arr[i]))
	       {
	           h.put(arr[i],h.get(arr[i])+1);
	       }
	       else
	       {
	           h.put(arr[i],1);
	       }
	   }
	   System.out.println(h);
	   
	   
	}
}
ooutput;
{20=2, 40=1, 10=2, 30=1}
