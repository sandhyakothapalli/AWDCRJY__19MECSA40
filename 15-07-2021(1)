import java.util.*;
class Arraylist
{
public static void main (String args[])
{
Scanner sc=new Scanner(System.in);
ArrayList<Integer>a=new ArrayList<Integer>();
a.add(10);
a.add(20);
a.add(30);
ArrayList<Integer>b=new ArrayList<Integer>();
b.add(40);
b.add(50);
b.add(60);
a.addAll(b);
for(int i=0;i<a.size();i++)
{
System.out.print(a.get(i)+ " ");
}
}
}
OUTPUT:
10 20 30 40 50 60 
